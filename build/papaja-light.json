{
    "name": "Purple",
    "type": "light",
    "colors": {
        "editor.background": "#fafafa",
        "editor.foreground": "#27272a",
        "editorGroup.border": "#d4d4d8",
        "editorGroupHeader.border": "#00000000",
        "editorGroupHeader.tabsBorder": "#00000000",
        "editorLineNumber.activeForeground": "#a1a1aa",
        "editorLineNumber.foreground": "#e4e4e7",
        "editorGutter.addedBackground": "#4ade8033",
        "editorGutter.modifiedBackground": "#fb923c52",
        "keybindingLabel.background": "#00000000",
        "keybindingLabel.foreground": "#71717a",
        "keybindingLabel.border": "#71717a33",
        "keybindingLabel.bottomBorder": "#71717a33",
        "keybindingLabel.foregroundActive": "#71717a",
        "list.activeSelectionBackground": "#fe6a1122",
        "list.activeSelectionForeground": "#c63908",
        "list.inactiveFocusBackground": "#c63908",
        "list.inactiveSelectionBackground": "#e4e4e7",
        "list.highlightForeground": "#ef4e07",
        "list.focusHighlightForeground": "#ef4e07",
        "textLink.foreground": "#ef4e07",
        "panel.background": "#f4f4f5",
        "quickInput.background": "#f4f4f5",
        "sideBar.background": "#f4f4f5",
        "sideBar.border": "#d4d4d8",
        "sideBar.foreground": "#6b7280",
        "sideBarTitle.foreground": "#bbbbbb",
        "terminal.background": "#f4f4f5",
        "activityBar.background": "#f4f4f5",
        "activityBar.border": "#d4d4d8",
        "activityBar.activeBorder": "#ef4e07",
        "activityBar.foreground": "#52525b",
        "activityBarBadge.background": "#ef4e07",
        "activityBarBadge.foreground": "#f4f4f5",
        "focusBorder": "#00000000",
        "statusBar.background": "#fafafa",
        "statusBar.foreground": "#18181b",
        "statusBar.debuggingBackground": "#ef4e0733",
        "statusBarItem.remoteBackground": "#00000000",
        "statusBarItem.remoteForeground": "#18181b",
        "statusBar.border": "#d4d4d8",
        "tab.activeBorder": "#ef4e07",
        "tab.activeBorderTop": "#fafafa",
        "tab.activeBackground": "#fafafa",
        "tab.inactiveBackground": "#fafafa",
        "titleBar.activeBackground": "#f4f4f5",
        "titleBar.border": "#d4d4d8"
    },
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "annotation": {
            "foreground": "#ef4e07"
        },
        "class": {
            "foreground": "#5528d9"
        },
        "class.annotation": {
            "foreground": "#ef4e07",
            "fontStyle": ""
        },
        "enumMember": {
            "foreground": "#a46404"
        },
        "method": {
            "foreground": "#835cf6"
        },
        "method.defaultLibrary": {
            "foreground": "#835cf6"
        },
        "module": {
            "foreground": "#a46404"
        },
        "parameter": {
            "foreground": "#18181b"
        },
        "parameter.declaration": {
            "foreground": "#18181b"
        },
        "parameter.label": {
            "foreground": "#9d2e0f"
        },
        "property": {
            "foreground": "#251065"
        },
        "property.annotation": {
            "foreground": "#ef4e07",
            "fontStyle": "italic"
        },
        "variable.importPrefix": {
            "foreground": "#05a8aa"
        },
        "variable.defaultLibrary": {
            "foreground": "#a46404"
        },
        "method:dart": {
            "foreground": "#835cf6"
        },
        "selfParameter:python": {
            "foreground": "#ef4e07",
            "fontStyle": "italic"
        }
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#a1a1aa"
            }
        },
        {
            "scope": [
                "comment.block.documentation",
                "comment.block.documentation storage",
                "comment.block.documentation storage.type",
                "comment.block.documentation keyword",
                "comment.block.documentation keyword.other.type",
                "comment.block.documentation punctuation",
                "comment.block.documentation punctuation.definition",
                "comment.block.documentation punctuation.separator",
                "comment.block.documentation punctuation.separator.delimiter",
                "comment.block.documentation variable",
                "comment.block.documentation entity",
                "comment.block.documentation support.class",
                "comment.block.documentation entity.name.type"
            ],
            "settings": {
                "foreground": "#a1a1aa",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type"
            ],
            "settings": {
                "foreground": "#ef4e07"
            }
        },
        {
            "scope": [
                "storage"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#fe6a11"
            }
        },
        {
            "scope": [
                "meta.namespace keyword",
                "meta.namespace entity.name.type",
                "meta.namespace punctuation.separator"
            ],
            "settings": {
                "foreground": "#fe6a11"
            }
        },
        {
            "scope": [
                "storage.type.function.arrow"
            ],
            "settings": {
                "foreground": "#ef4e07",
                "fontStyle": "normal"
            }
        },
        {
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#507318"
            }
        },
        {
            "scope": [
                "constant",
                "constant.numeric",
                "constant punctuation.separator",
                "constant.numeric keyword.other.unit",
                "support.constant",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#ce8d00"
            }
        },
        {
            "scope": [
                "punctuation.dot",
                "punctuation.accessor",
                "punctuation.definition.variable",
                "punctuation.definition.arrow",
                "meta.function.php punctuation.separator.delimiter",
                "entity.name.tag.yaml",
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#ef4e07"
            }
        },
        {
            "scope": [
                "punctuation",
                "meta.brace",
                "begin.bracket.round.blade",
                "end.bracket.round.blade",
                "meta.tag.tsx keyword.operator.assignment",
                "meta.tag.jsx keyword.operator.assignment",
                "meta.function.parameters.php punctuation.separator.delimiter",
                "source.css keyword.operator",
                "source.css entity.name.tag"
            ],
            "settings": {
                "foreground": "#71717a"
            }
        },
        {
            "name": "Imports",
            "scope": [
                "meta.import",
                "meta.import keyword.control",
                "meta.export keyword.control",
                "keyword.control.import",
                "keyword.other.import",
                "meta.preprocessor.include",
                "meta.declaration.dart"
            ],
            "settings": {
                "foreground": "#ef4e07",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "support.class",
                "entity.name.type",
                "entity.name.class",
                "entity.other.inherited-class",
                "keyword.other.type",
                "support.type.primitive",
                "variable.language.this",
                "keyword.operator.nullable-type.php",
                "source.js meta.import variable.other.readwrite",
                "source.jsx meta.import variable.other.readwrite",
                "source.ts meta.import variable.other.readwrite",
                "source.tsx meta.import variable.other.readwrite",
                "source.css entity"
            ],
            "settings": {
                "foreground": "#5528d9"
            }
        },
        {
            "scope": [
                "support.function",
                "entity.name.function",
                "punctuation.definition.variable.php",
                "meta.template.expression punctuation",
                "variable.language.this.php",
                "support.function.construct"
            ],
            "settings": {
                "foreground": "#835cf6"
            }
        },
        {
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#18181b"
            }
        },
        {
            "scope": [
                "variable.other.property",
                "support.variable.property"
            ],
            "settings": {
                "foreground": "#251065"
            }
        },
        {
            "name": "Super, Self, Me, etc.",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#ef4e07",
                "fontStyle": "italic"
            }
        },
        {
            "name": "this",
            "scope": [
                "variable.language.this"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.use",
                "meta.import",
                "meta.namespace",
                "source.php meta.function.closure storage.type",
                "source.php meta.function.closure keyword"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.use punctuation.separator",
                "meta.use support.other"
            ],
            "settings": {
                "foreground": "#4721b688"
            }
        },
        {
            "name": "Enum values: Enum::FooBar",
            "scope": [
                "constant.other.class.php"
            ],
            "settings": {
                "foreground": "#a46404"
            }
        },
        {
            "name": "Named arguments foo(bar: 1)",
            "scope": [
                "meta.function-call.php entity.name.variable.parameter",
                "meta.function-call.php punctuation.separator.colon"
            ],
            "settings": {
                "foreground": "#9d2e0f"
            }
        },
        {
            "name": "PHP Attributes #[Route]",
            "scope": [
                "meta.attribute.php",
                "meta.attribute.php punctuation.definition",
                "meta.attribute.php support.attribute"
            ],
            "settings": {
                "foreground": "#008EE0"
            }
        },
        {
            "name": "Tag names",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#4721b6"
            }
        },
        {
            "name": "Tag names Chevrons in React",
            "scope": [
                "source.jsx punctuation.definition.tag",
                "source.tsx punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#4721b688"
            }
        },
        {
            "name": "Tag names Chevrons",
            "scope": [
                "text.html punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#4721b688"
            }
        },
        {
            "name": "Tag Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#c63908",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Custom Tags: <x-foo>",
            "scope": [
                "text.html meta.tag.custom entity.name.tag"
            ],
            "settings": {
                "foreground": "#006199"
            }
        },
        {
            "name": "Custom Tags Chevrons: <x-foo>",
            "scope": [
                "text.html meta.tag.custom punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#00619988"
            }
        },
        {
            "name": "Custom Tag Attributes",
            "scope": [
                "meta.tag.custom entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#c63908",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Blade instructions @foo / + <?php ?>",
            "scope": [
                "text.html keyword.blade",
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php"
            ],
            "settings": {
                "foreground": "#c63908"
            }
        },
        {
            "name": "Property Names - 'foo': bar",
            "scope": [
                "source.css meta.property-name support.type.property-name"
            ],
            "settings": {
                "foreground": "#9d2e0f"
            }
        },
        {
            "name": "Class names",
            "scope": [
                "source.css entity.other.attribute-name.class"
            ],
            "settings": {
                "foreground": "#5528d9"
            }
        },
        {
            "name": "html attribute names (type, ...)",
            "scope": [
                "source.css entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#c63908",
                "fontStyle": "normal"
            }
        },
        {
            "name": "CSS Property Values",
            "scope": [
                "source.css meta.property-value"
            ],
            "settings": {
                "foreground": "#d4d4d8"
            }
        },
        {
            "name": "CSS Property Values: Constant colors",
            "scope": [
                "source.css constant.other.color",
                "source.css constant.other.color punctuation.definition"
            ],
            "settings": {
                "foreground": "#ef4e07"
            }
        },
        {
            "name": "CSS Property Values - Function call and @at rules",
            "scope": [
                "source.css support.function",
                "source.css keyword.control.at-rule",
                "source.css keyword.control.at-rule punctuation.definition",
                "meta.at-rule.tailwind",
                "meta.at-rule.tailwind punctuation.definition",
                "meta.at-rule.tailwind keyword.control",
                "meta.at-rule.tailwind variable"
            ],
            "settings": {
                "foreground": "#ef4e07"
            }
        },
        {
            "scope": [
                "source.css support.constant"
            ],
            "settings": {
                "foreground": "#ef4e07"
            }
        },
        {
            "scope": [
                "source.css entity.name.tag"
            ],
            "settings": {
                "foreground": "#52525b"
            }
        },
        {
            "name": "@apply content",
            "scope": [
                "source.css meta.at-rule.apply entity.other.attribute-name.class"
            ],
            "settings": {
                "foreground": "#5528d9",
                "fontStyle": ""
            }
        },
        {
            "name": "CSS Variables",
            "scope": [
                "source.css variable"
            ],
            "settings": {
                "foreground": "#18181b",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Variables",
            "scope": [
                "source.css comment"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Imports",
            "scope": [
                "source.c meta.preprocessor",
                "source.c meta.preprocessor keyword.control",
                "source.c meta.preprocessor punctuation.definition",
                "source.python keyword.control.import",
                "meta.declaration keyword.other.import"
            ],
            "settings": {
                "foreground": "#ef4e07",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Arguments in function",
            "scope": [
                "source.python meta.function-call.arguments"
            ],
            "settings": {
                "foreground": "#18181b"
            }
        }
    ]
}