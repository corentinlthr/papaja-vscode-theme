            "settings": {
                "foreground": "#ffb571"
            }
        },
        {
            "name": "CSS Property Values - Constants",
            "scope": [
                "source.css support.constant"
            ],
            "settings": {
                "foreground": "#ffb571"
            }
        },
        {
            "name": "CSS Selectors - Entity",
            "scope": [
                "source.css entity"
            ],
            "settings": {
                "foreground": "#A394E5"
            }
        },
        {
            "name": "CSS Selectors - Entity",
            "scope": [
                "source.css entity.name.tag"
            ],
            "settings": {}
        },
        {
            "name": "@apply content",
