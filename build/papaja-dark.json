        },
        {
            "name": "CSS Variables",
            "scope": [
                "source.css variable"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Variables",
            "scope": [
                "source.css comment"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Imports",
            "scope": [
                "source.c meta.preprocessor",
                "source.c meta.preprocessor keyword.control",
                "source.c meta.preprocessor punctuation.definition",
                "source.python keyword.control.import",
                "meta.declaration keyword.other.import"
            ],
