{
    "name": "Purple",
    "type": "dark",
    "colors": {
        "editor.background": "#18181b",
        "editor.foreground": "#e4e4e7",
        "editorLineNumber.activeForeground": "#3f3f46",
        "editorLineNumber.foreground": "#09090b00",
        "editorGutter.addedBackground": "#4ade8033",
        "editorGutter.modifiedBackground": "#fb923c52",
        "list.activeSelectionBackground": "#c084fc33",
        "list.activeSelectionForeground": "#d4d4d8",
        "list.hoverBackground": "#18181b",
        "list.hoverForeground": "#d4d4d8",
        "list.inactiveFocusBackground": "#1d2d3e",
        "list.inactiveSelectionBackground": "#18181b",
        "list.inactiveSelectionForeground": "#d4d4d8",
        "panel.background": "#09090b",
        "quickInput.background": "#27272a",
        "sideBar.background": "#09090b",
        "sideBar.border": "#27272a",
        "sideBar.foreground": "#6b7280",
        "sideBarTitle.foreground": "#bbbbbb",
        "terminal.background": "#09090b",
        "activityBar.background": "#09090b",
        "activityBar.border": "#27272a",
        "activityBar.activeBorder": "#fffac2",
        "activityBar.foreground": "#fffac2",
        "activityBarBadge.background": "#fffac2",
        "activityBarBadge.foreground": "#000000",
        "focusBorder": "#00000000",
        "statusBar.background": "#835cf633",
        "titleBar.activeBackground": "#09090b",
        "titleBar.border": "#27272a"
    },
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "annotation": {
            "foreground": "#ffb571"
        },
        "class": {
            "foreground": "#A394E5"
        },
        "class.annotation": {
            "foreground": "#ffb571",
            "fontStyle": ""
        },
        "enumMember": {
            "foreground": "#fff387"
        },
        "method": {
            "foreground": "#c7b5fd"
        },
        "method.defaultLibrary": {
            "foreground": "#c7b5fd"
        },
        "module": {
            "foreground": "#fffac2"
        },
        "parameter": {
            "foreground": "#e4e4e7"
        },
        "parameter.declaration": {
            "foreground": "#e4e4e7"
        },
        "parameter.label": {
            "foreground": "#ffd4a8"
        },
        "property": {
            "foreground": "#e0d6fe"
        },
        "property.annotation": {
            "foreground": "#ffb571",
            "fontStyle": "italic"
        },
        "variable.importPrefix": {
            "foreground": "#93fcf3"
        },
        "variable.defaultLibrary": {
            "foreground": "#fffac2"
        },
        "method:dart": {
            "foreground": "#c7b5fd"
        },
        "selfParameter:python": {
            "foreground": "#ffb571",
            "fontStyle": "italic"
        }
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#52525b"
            }
        },
        {
            "scope": [
                "comment.block.documentation",
                "comment.block.documentation storage",
                "comment.block.documentation storage.type",
                "comment.block.documentation keyword",
                "comment.block.documentation keyword.other.type",
                "comment.block.documentation punctuation",
                "comment.block.documentation punctuation.definition",
                "comment.block.documentation punctuation.separator",
                "comment.block.documentation punctuation.separator.delimiter",
                "comment.block.documentation variable",
                "comment.block.documentation entity",
                "comment.block.documentation support.class",
                "comment.block.documentation entity.name.type"
            ],
            "settings": {
                "foreground": "#52525b",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type"
            ],
            "settings": {
                "foreground": "#ffb571"
            }
        },
        {
            "scope": [
                "storage"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ffd4a8"
            }
        },
        {
            "scope": [
                "meta.namespace keyword",
                "meta.namespace entity.name.type",
                "meta.namespace punctuation.separator"
            ],
            "settings": {
                "foreground": "#ffd4a8"
            }
        },
        {
            "scope": [
                "storage.type.function.arrow"
            ],
            "settings": {
                "foreground": "#ffb571",
                "fontStyle": "normal"
            }
        },
        {
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#aad94c"
            }
        },
        {
            "scope": [
                "constant",
                "constant.numeric",
                "constant punctuation.separator",
                "constant.numeric keyword.other.unit",
                "support.constant",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#fff387"
            }
        },
        {
            "scope": [
                "punctuation.dot",
                "punctuation.accessor",
                "punctuation.definition.variable",
                "punctuation.definition.arrow",
                "meta.function.php punctuation.separator.delimiter",
                "entity.name.tag.yaml",
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#ffb571"
            }
        },
        {
            "scope": [
                "punctuation",
                "meta.brace",
                "begin.bracket.round.blade",
                "end.bracket.round.blade",
                "meta.tag.tsx keyword.operator.assignment",
                "meta.tag.jsx keyword.operator.assignment",
                "meta.function.parameters.php punctuation.separator.delimiter"
            ],
            "settings": {
                "foreground": "#a1a1aa"
            }
        },
        {
            "name": "Imports",
            "scope": [
                "meta.import",
                "meta.import keyword.control",
                "meta.export keyword.control",
                "keyword.control.import",
                "keyword.other.import",
                "meta.preprocessor.include",
                "meta.declaration.dart"
            ],
            "settings": {
                "foreground": "#ffb571",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "support.class",
                "entity.name.type",
                "entity.name.class",
                "entity.other.inherited-class",
                "keyword.other.type",
                "support.type.primitive",
                "variable.language.this",
                "keyword.operator.nullable-type.php",
                "source.js meta.import variable.other.readwrite",
                "source.jsx meta.import variable.other.readwrite",
                "source.ts meta.import variable.other.readwrite",
                "source.tsx meta.import variable.other.readwrite"
            ],
            "settings": {
                "foreground": "#A394E5"
            }
        },
        {
            "scope": [
                "support.function",
                "entity.name.function",
                "punctuation.definition.variable.php",
                "meta.template.expression punctuation",
                "variable.language.this.php",
                "support.function.construct"
            ],
            "settings": {
                "foreground": "#c7b5fd"
            }
        },
        {
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#e4e4e7"
            }
        },
        {
            "scope": [
                "variable.other.property",
                "support.variable.property"
            ],
            "settings": {
                "foreground": "#e0d6fe"
            }
        },
        {
            "name": "Super, Self, Me, etc.",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#ffb571",
                "fontStyle": "italic"
            }
        },
        {
            "name": "this",
            "scope": [
                "variable.language.this"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.use",
                "meta.import",
                "meta.namespace",
                "source.php meta.function.closure storage.type",
                "source.php meta.function.closure keyword"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.use punctuation.separator",
                "meta.use support.other"
            ],
            "settings": {
                "foreground": "#c7b5fd55"
            }
        },
        {
            "name": "Enum values: Enum::FooBar",
            "scope": [
                "constant.other.class.php"
            ],
            "settings": {
                "foreground": "#fff387"
            }
        },
        {
            "name": "Named arguments foo(bar: 1)",
            "scope": [
                "meta.function-call.php entity.name.variable.parameter",
                "meta.function-call.php punctuation.separator.colon"
            ],
            "settings": {
                "foreground": "#ffd4a8"
            }
        },
        {
            "name": "PHP Attributes #[Route]",
            "scope": [
                "meta.attribute.php",
                "meta.attribute.php punctuation.definition",
                "meta.attribute.php support.attribute"
            ],
            "settings": {
                "foreground": "#94D8FF"
            }
        },
        {
            "name": "Tag names",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#A394E5"
            }
        },
        {
            "name": "Tag names Chevrons in React",
            "scope": [
                "source.js punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#A394E555"
            }
        },
        {
            "name": "Tag names Chevrons",
            "scope": [
                "text.html punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#A394E555"
            }
        },
        {
            "name": "Tag Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ffb571",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Custom Tags: <x-foo>",
            "scope": [
                "text.html meta.tag.custom entity.name.tag"
            ],
            "settings": {
                "foreground": "#94D8FF"
            }
        },
        {
            "name": "Custom Tags Chevrons: <x-foo>",
            "scope": [
                "text.html meta.tag.custom punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#94D8FF55"
            }
        },
        {
            "name": "Custom Tag Attributes",
            "scope": [
                "meta.tag.custom entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#CCECFF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Blade instructions @foo / + <?php ?>",
            "scope": [
                "text.html keyword.blade",
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php"
            ],
            "settings": {
                "foreground": "#ff8b38"
            }
        },
        {
            "name": "Property Names - 'foo': bar",
            "scope": [
                "source.css meta.property-name support.type.property-name"
            ],
            "settings": {
                "foreground": "#ffd4a8"
            }
        },
        {
            "name": "Class names",
            "scope": [
                "source.css entity.other.attribute-name.class"
            ],
            "settings": {
                "foreground": "#ADE1FF"
            }
        },
        {
            "name": "HTML elements",
            "scope": [
                "source.css entity.name.tag"
            ],
            "settings": {
                "foreground": "#A394E5"
            }
        },
        {
            "name": "CSS Property Values",
            "scope": [
                "source.css meta.property-value"
            ],
            "settings": {
                "foreground": "#d4d4d8"
            }
        },
        {
            "name": "CSS Property Values: Constant colors",
            "scope": [
                "source.css constant.other.color",
                "source.css constant.other.color punctuation.definition"
            ],
            "settings": {
                "foreground": "#ffb571"
            }
        },
        {
            "name": "CSS Property Values - Function call and @at rules",
            "scope": [
                "source.css support.function",
                "source.css keyword.control.at-rule",
                "source.css keyword.control.at-rule punctuation.definition",
                "meta.at-rule.tailwind",
                "meta.at-rule.tailwind punctuation.definition",
                "meta.at-rule.tailwind keyword.control",
                "meta.at-rule.tailwind variable"
