{
    // zinc-1000: #121214
    // zinc-900: #18181b
    // zinc-800: #27272a
    // zinc-700 #3f3f46


    "name": "Papaja Theme",
    "type": "dark",
    "colors":   {
        "activityBar.activeBorder": "#fdba74",
        "activityBar.activeFocusBorder": "#fdba74",
        "activityBar.background": "#18181b",
        "activityBar.border": "#3f3f46",
        "activityBar.foreground": "#d4d4d8",
        "activityBarBadge.background": "#c084fc",
        "activityBarBadge.foreground": "#fff",
        "editor.background": "#18181b",
        "editor.foldBackground": "#1f293733",
        "editor.foreground": "#d4d4d8",
        "editorGutter.addedBackground": "#4ade8033",
        "editorGutter.modifiedBackground": "#fb923c52",
        "editorLineNumber.activeForeground": "#3f3f33",
        "editorLineNumber.foreground": "#3f3f4600",
        "focusBorder": "#c084fc77",
        "list.activeSelectionBackground": "#c084fc33",
        "list.activeSelectionForeground": "#d4d4d8",
        "list.hoverBackground": "#18181b",
        "list.hoverForeground": "#d4d4d8",
        "list.inactiveFocusBackground": "#1d2d3e",
        "list.inactiveSelectionBackground": "#18181b",
        "list.inactiveSelectionForeground": "#d4d4d8",
        "panel.background": "#121214",
        "quickInput.background": "#121214",
        "sideBar.background": "#121214",
        "sideBar.border": "#3f3f46",
        "sideBar.foreground": "#6b7280",
        "sideBarTitle.foreground": "#bbbbbb",
        "terminal.background": "#121214",
    },
    "semanticHighlighting": true,
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#ffffff44"
            }
        },
        {
            "name": "Documentation",
            "scope": [
                "comment.block.documentation keyword",
                "comment.block.documentation support.class",
                "comment.block.documentation variable",
                "comment.block.documentation",
                "entity.name.type.instance.jsdoc",
                "keyword.other.phpdoc",
                "meta.other.type.phpdoc support.other.namespace",
                "meta.other.type.phpdoc punctuation.separator.delimiter.php",
                "meta.other.type.phpdoc keyword.other.type",
                "punctuation.definition.block.tag.jsdoc",
                "punctuation.definition.bracket.curly.begin.jsdoc",
                "punctuation.definition.bracket.curly.end.jsdoc",
                "storage.type.class.jsdoc",
                "variable.other.jsdoc",
                "meta.other.type.phpdoc support.other.namespace punctuation.separator.inheritance.php",
                "text.html.php punctuation.section.embedded.begin.php",
                "text.html.php punctuation.section.embedded.end.php",
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ffffff44"
            }
        },
        {
            "scope": [
                "variable",
            ],
            "settings": {
                "foreground": "#d4d4d8"
            }
        },
        {
            "scope": [
                "variable punctuation.definition.variable.php",
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#7dd3fc"
            }
        },
        {
            "scope": [
                "support.other.namespace",
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ffffff55"
            }
        },
        {
            "scope": [
                "source.js punctuation.definition.tag", // JSX tags < >
                "meta.tag.other punctuation.definition", // < > for invalid html tags
            ],
            "settings": {
                "foreground": "#ffffff55"
            }
        },
        {
            "scope": [
                "meta.namespace",
                "meta.function.closure storage.type"
            ],
            "settings": {
                "fontStyle": "italic",
            }
        },


        // MARK: purple-400

        {
            "scope": [

            ],
            "settings": {
                "foreground": "#c084fc"
            }
        },

        {
            "scope": [
                "meta.namespace entity.name.type.namespace",
                "meta.namespace entity.name.type.namespace punctuation.separator",
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#c084fc"
            }
        },
        {
            "scope": [
                "text.html.php punctuation.section.embedded.begin.php",
                "text.html.php punctuation.section.embedded.end.php"
            ],
            "settings": {
                "foreground": "#c084fc44"
            }
        },


        // MARK:  orange-300

        {
            "scope": [
                "keyword.control.at-rule.import punctuation.definition",
                "keyword.control.export",
                "keyword.control.return",
                "keyword.other.type",
                "meta.at-rule.import keyword.control.at-rule.import",
                "meta.import keyword",
                "meta.namespace.php",
                "meta.use keyword",
                "storage",
                "keyword.operator",
                "storage.type.function.arrow.js",
                "keyword.type.graphql",
                "keyword.input.graphql",
                "punctuation.accessor.js",
                "meta.object.member punctuation.accessor",
                "variable.parameter.tailwind",
                "meta.structure.dictionary.value.json",
                "string.quoted.double.json",
                "punctuation.definition.string.begin.json",
                "punctuation.definition.string.end.json",
            ],
            "settings": {
                "foreground": "#fdba74"
            }
        },

        {
            "scope": [
                "meta.attribute entity.other.attribute-name",
                "meta.tag.attributes entity.other.attribute-name",
                "support.type.property-name",
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#fdba74"
            }
        },



        // MARK: sky-300

        {
            "scope": [
                "begin.bracket.round",
                "constant",
                "end.bracket.round",
                "meta.tag.structure entity.name.tag.html",
                "meta.tag.inline entity.name.tag.html",
                "entity.name",
                "entity.other.inherited-class",
                "meta.brace.round",
                "meta.class",
                "meta.function-call.js",
                "meta.function-call",
                "meta.function.echo.blade source",
                "meta.function.parameter.typehinted support.class",
                "meta.import variable.other.readwrite.alias",
                "meta.tag.attributes keyword.operator",
                "meta.tag.other punctuation.definition.variable",
                "meta.use support.other.namespace",
                "punctuation.definition",
                "punctuation.section.array",
                "punctuation.section",
                "punctuation.separator",
                "punctuation.terminator",
                "support.class",
                "support.function.js",
                "support.function.misc.scss",
                "support.type.graphql",
                "meta.function-call",
                "support.function.js",
                "meta.function-call.js",
                "meta.method-call",
            ],
            "settings": {
                "foreground": "#7dd3fc"
            }
        },

        {
            "scope": [
                "variable.language.this.php",
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#7dd3fc"
            }
        },

        {
            "scope": [
                "meta.method-call.php punctuation.separator.colon",
                "meta.method-call.php entity.name.variable.parameter",
                "meta.function-call.php punctuation.separator.colon",
                "meta.function-call.php entity.name.variable.parameter"
            ],
            "settings": {
                "foreground": "#7dd3fc66"
            }
        },

        {
            "scope": [
                "meta.function.echo.blade",
                "meta.function.parameter.typehinted support.other.namespace",
                "meta.import punctuation.definition",
                "meta.tag.structure punctuation.definition.tag", // HTML entities
                "meta.tag.inline punctuation.definition.tag", // HTML entities
                "support.other.namespace",
            ],
            "settings": {
                "foreground": "#7dd3fc55"
            }
        },

        {
            "scope": [
                "meta.use support.other.namespace",
                "support.other.namespace punctuation.separator.inheritance.php"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#7dd3fc55"
            }
        },


        // MARK: purple-300

        {
            "scope": [
                "keyword",
                "constant.language",
                "entity.name support.class.component",
                "entity.other.attribute-name",
                "meta.tag.custom entity.name.tag.html", // HTML custom component name
                "constant.language.php",
                "string constant.character.escape",
                "meta.object-literal.key",
                "variable.object.property.js",
                "variable.parameter.graphql",
                "support.type.object.module",
                "variable.other.object.property.js",
                "constant"
            ],
            "settings": {
                "foreground": "#d8b4fe"
            }
        },

        {
            "scope": [
                "meta.tag.custom punctuation.definition.tag", // HTML custom component < `
            ],
            "settings": {
                "foreground": "#d8b4fe55"
            }
        },

        // MARK: amber-100

        {
            "scope": [
                "meta.directive.blade",
                "keyword.blade",
                "text.html.php.blade punctuation.section.embedded.begin.php",
                "text.html.php.blade punctuation.section.embedded.end.php",

                "keyword.control.at-rule",
                "keyword.control.at-rule punctuation.definition",
            ],
            "settings": {
                "foreground": "#fef3c7"
            }
        },

        // MARK: pink-300

        {
            "scope": [
                "meta.tag.other entity.name.tag",

            ],
            "settings": {
                "foreground": "#f9a8d4"
            }
        },



        // MARK: JSON

        {
            "scope": [
                "support.type.property-name.json" // JSON key
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#cffafe"
            }
        },
        {
            "scope": [
                "source.json punctuation",
                "source.json punctuation.separator",
                "source.json punctuation.definition",
                "support.type.property-name.json punctuation",
                "string.quoted.double.json punctuation",
                "source.json punctuation.definition.string"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#71717a"
            }
        },

        {
            "scope": [
                "string.quoted.double.json", // JSON string
            ],
            "settings": {
                "foreground": "#fdba74"
            }
        },

        // MARK: String

        {
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#86efac"
            }
        },







        {
            "name": "Constants",
            "scope": [
                "constant.other.php",
                "constant.other.class",
                "variable.other.property.js",
            ],
            "settings": {
                "foreground": "#a5b4fc"
            }
        },




        // HTML

        {
            "name": "HTML Entities",
            "scope": [
                "punctuation.definition.entity",
                "constant.character.entity",
            ],
            "settings": {
                "foreground": "#a5b4fc"
            }
        },


    ]
}
