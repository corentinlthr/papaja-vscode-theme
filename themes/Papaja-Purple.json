{
    "name": "Purple",
    "type": "dark",
    "semanticHighlighting": false,
    "colors": {
        "editor.background": "#18181b",
        "editor.foreground": "#e4e4e7",
        "editorLineNumber.activeForeground": "#3f3f46",
        "editorLineNumber.foreground": "#09090b00",
        "editorGutter.addedBackground": "#4ade8033",
        "editorGutter.modifiedBackground": "#fb923c52",
        "list.activeSelectionBackground": "#c084fc33",
        "list.activeSelectionForeground": "#d4d4d8",
        "list.hoverBackground": "#18181b",
        "list.hoverForeground": "#d4d4d8",
        "list.inactiveFocusBackground": "#1d2d3e",
        "list.inactiveSelectionBackground": "#18181b",
        "list.inactiveSelectionForeground": "#d4d4d8",
        "panel.background": "#09090b",
        "quickInput.background": "#27272a",
        "sideBar.background": "#09090b",
        "sideBar.border": "#27272a",
        "sideBar.foreground": "#6b7280",
        "sideBarTitle.foreground": "#bbbbbb",
        "terminal.background": "#09090b",
        "activityBar.background": "#09090b",
        "activityBar.border": "#27272a",
        "activityBar.activeBorder": "#fff8c2",
        "activityBar.foreground": "#fff8c2",
        "activityBarBadge.background": "#fff8c2",
        "activityBarBadge.foreground": "#000000",
        "focusBorder": "#00000000",
        "statusBar.background": "#a78bfa33",
        "titleBar.activeBackground": "#09090b",
        "titleBar.border": "#27272a"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#52525b"
            }
        },
        {
            "scope": [
                "comment.block.documentation",
                "comment.block.documentation storage",
                "comment.block.documentation keyword",
                "comment.block.documentation punctuation",
                "comment.block.documentation punctuation.definition",
                "comment.block.documentation punctuation.separator",
                "comment.block.documentation variable",
                "comment.block.documentation entity",
                "comment.block.documentation support.class"
            ],
            "settings": {
                "foreground": "#52525b",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword",
                "storage",
                "keyword.control.flow",
                "keyword.control.return",
                "punctuation.accessor",
                "punctuation.definition.arrow",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#ffb571"
            }
        },
        {
            "scope": [
                "keyword.control"
            ],
            "settings": {
                "foreground": "#fff8c2"
            }
        },
        {
            "scope": [
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#ffd4a8"
            }
        },
        {
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#e4e4e7"
            }
        },
        {
            "scope": [
                "variable.language.this",
                "variable.language.this punctuation.definition.variable"
            ],
            "settings": {
                "foreground": "#a394e5",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#ffb571",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "punctuation",
                "punctuation.definition.variable"
            ],
            "settings": {
                "foreground": "#a394e5"
            }
        },
        {
            "scope": [
                "punctuation.definition",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.section",
                "meta.brace",
                "meta.tag.tsx keyword.operator.assignment",
                "meta.tag.jsx keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#a1a1aa"
            }
        },
        {
            "scope": [
                "meta.method-call",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#a394e5"
            }
        },
        {
            "scope": [
                "support.class",
                "keyword.operator.nullable-type"
            ],
            "settings": {
                "foreground": "#a78bfa"
            }
        },
        {
            "scope": [
                "meta.class entity.name",
                "meta.class entity.other.inherited-class",
                "entity.name.type.class"
            ],
            "settings": {
                "foreground": "#a78bfa"
            }
        },
        {
            "scope": [
                "meta.class meta.definition.method",
                "meta.class meta.function entity.name",
                "meta.class meta.function support.function"
            ],
            "settings": {
                "foreground": "#a394e5",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "meta.function-call",
                "meta.function-call entity.name.function",
                "meta.method-call entity.name.function"
            ],
            "settings": {
                "foreground": "#a394e5",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity"
            ],
            "settings": {
                "foreground": "#a394e5"
            }
        },
        {
            "scope": [
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#e0d6fe"
            }
        },
        {
            "scope": [
                "constant",
                "constant punctuation.separator"
            ],
            "settings": {
                "foreground": "#97bbff"
            }
        },
        {
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#86efac"
            }
        },
        {
            "scope": [
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#86efac"
            }
        },
        {
            "scope": [
                "meta.use",
                "meta.namespace",
                "meta.function.closure storage.type"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.use punctuation.separator",
                "meta.use support.other"
            ],
            "settings": {
                "foreground": "#a394e555"
            }
        },
        {
            "scope": [
                "support.function.construct"
            ],
            "settings": {
                "foreground": "#a394e5"
            }
        },
        {
            "scope": [
                "meta.namespace keyword",
                "meta.namespace entity.name",
                "meta.namespace punctuation.separator"
            ],
            "settings": {
                "foreground": "#ffd4a8"
            }
        },
        {
            "name": "Named arguments foo(bar: 1)",
            "scope": [
                "meta.function-call.php entity.name.variable.parameter"
            ],
            "settings": {
                "foreground": "#a394e588"
            }
        },
        {
            "name": "PHP Attributes #[Route]",
            "scope": [
                "meta.attribute.php"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "PHP Attributes name #[$foo]",
            "scope": [
                "meta.attribute.php support.attribute"
            ],
            "settings": {
                "foreground": "#a78bfa"
            }
        },
        {
            "name": "Tag names",
            "scope": [
                "text.html entity.name.tag"
            ],
            "settings": {
                "foreground": "#a394e5"
            }
        },
        {
            "name": "Tag names",
            "scope": [
                "text.html punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#a394e555"
            }
        },
        {
            "name": "Custom Tags: <x-foo>",
            "scope": [
                "text.html meta.tag.custom entity.name.tag"
            ],
            "settings": {
                "foreground": "#a394e5"
            }
        },
        {
            "name": "Custom Tags: <x-foo>",
            "scope": [
                "text.html meta.tag.custom punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#a394e555"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ffd4a8"
            }
        },
        {
            "name": "Attribute names",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Blade instructions @foo / + <?php ?>",
            "scope": [
                "text.html keyword.blade",
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php"
            ],
            "settings": {
                "foreground": "#ffb571"
            }
        },
        {
            "name": "Parenthesis in Blade instructions @foo()",
            "scope": [
                "text.html.php.blade begin.bracket.round",
                "text.html.php.blade end.bracket.round"
            ],
            "settings": {
                "foreground": "#a1a1aa"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "source.json string support.type.property-name"
            ],
            "settings": {
                "foreground": "#bab3ee"
            }
        },
        {
            "name": "JS Object keys",
            "scope": [
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#bab3ee"
            }
        },
        {
            "name": "JSON Punctuation",
            "scope": [
                "source.json punctuation",
                "source.json punctuation.definition",
                "source.json punctuation.separator"
            ],
            "settings": {
                "foreground": "#a1a1aa"
            }
        },
        {
            "name": "JS Class/variable imports",
            "scope": [
                "source.js meta.import variable.other.readwrite",
                "source.jsx meta.import variable.other.readwrite",
                "source.ts meta.import variable.other.readwrite",
                "source.tsx meta.import variable.other.readwrite"
            ],
            "settings": {
                "foreground": "#a78bfa"
            }
        },
        {
            "name": "CSS Property Names",
            "scope": [
                "source.css meta.property-name"
            ],
            "settings": {
                "foreground": "#ffed89"
            }
        },
        {
            "name": "CSS Property Values",
            "scope": [
                "source.css meta.property-value"
            ],
            "settings": {
                "foreground": "#d4d4d8"
            }
        },
        {
            "name": "CSS Property Values - Function call",
            "scope": [
                "source.css meta.property-value meta.function"
            ],
            "settings": {
                "foreground": "#a394e5"
            }
        },
        {
            "name": "CSS Property Values - Constants",
            "scope": [
                "source.css support.constant"
            ],
            "settings": {
                "foreground": "#ffd4a8"
            }
        },
        {
            "name": "CSS Selectors - Entity",
            "scope": [
                "source.css entity"
            ],
            "settings": {
                "foreground": "#a78bfa"
            }
        },
        {
            "name": "CSS Selectors - Attributes (classes, ids, etc.)",
            "scope": [
                "source.css entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#e0d6fe",
                "fontStyle": ""
            }
        },
        {
            "name": "CSS Selectors - Attributes selector ([type='foo'])",
            "scope": [
                "source.css meta.attribute-selector entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ffd4a8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Selectors - Punctuation in selectors",
            "scope": [
                "source.css meta.selector punctuation.definition",
                "source.css meta.selector keyword"
            ],
            "settings": {
                "foreground": "#e4e4e7",
                "fontStyle": ""
            }
        },
        {
            "name": "@tailwind",
            "scope": [
                "meta.at-rule.tailwind",
                "meta.at-rule.tailwind punctuation.definition",
                "meta.at-rule.tailwind keyword.control",
                "meta.at-rule.tailwind variable"
            ],
            "settings": {
                "foreground": "#ffb571"
            }
        },
        {
            "name": "@apply @layer",
            "scope": [
                "source.css keyword.control.at-rule",
                "source.css keyword.control.at-rule punctuation.definition"
            ],
            "settings": {
                "foreground": "#ffb571"
            }
        },
        {
            "name": "@apply content",
            "scope": [
                "source.css meta.at-rule.apply entity"
            ],
            "settings": {
                "foreground": "#e0d6fe"
            }
        },
        {
            "name": "CSS Variables",
            "scope": [
                "source.css variable"
            ],
            "settings": {
                "foreground": "#d4d4d8"
            }
        }
    ]
}